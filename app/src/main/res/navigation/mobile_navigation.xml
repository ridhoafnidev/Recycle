<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home"
    tools:ignore="UnusedNavigation">

    <fragment
        android:id="@+id/navigation_home"
        android:name="andre.recycle.Fragment.HomeFragment"
        android:label="Home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_riwayat"
        android:name="andre.recycle.Fragment.RiwayatFragment"
        android:label="Riwayat"
        tools:layout="@layout/fragment_riwayat"/>

    <fragment
        android:id="@+id/navigation_informasi"
        android:name="andre.recycle.Fragment.InformasiFragment"
        android:label="Informasi"
        tools:layout="@layout/fragment_informasi" />

    <fragment
        android:id="@+id/navigation_akun"
        android:name="andre.recycle.Fragment.AkunFragment"
        android:label="Akun"
        tools:layout="@layout/fragment_akun" />
</navigation>